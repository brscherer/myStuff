<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="brscherer.github.com.mystuff">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserActivity"></activity>
        <activity android:name=".NewStuffActivity"></activity>
        <activity android:name=".GetStuffActivity"></activity>
        <activity android:name=".NewPersonActivity"></activity>
        <activity android:name=".BorrowListActivity"></activity>
        <activity android:name=".BorrowActivity"></activity>
        <activity android:name=".EditStuffActivity"></activity>
        <activity android:name=".EditPersonActivity"></activity>
    </application>

</manifest>